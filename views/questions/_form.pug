<h3>2. Create Tickets</h3>

<form-group>
  <label>LISTING PRIVACY</label>
  <div class="row">
    <div class="col-sm-3">
      <p></p>
      <p><button class="btn btn-primary" data-toggle="collapse" href="#collapseExample" aria-expanded="false" aria-controls="collapseExample"> FREE TICKET</button></p>
        <div class="collapse" id="collapseExample">
          <div class="well">
          <label for="exampleInputName1">Ticket name</label>
          <input type="text" class="form-control" id="exampleInputName1" placeholder="Jane Doe">
          <label for="exampleInputName2">Price</label>
          <input class="form-control" id="disabledInput" type="text" placeholder="Free" disabled>
          </div>
        </div>
      </div> 
    <div class="col-sm-3">
      <p></p>
      <p><button class="btn btn-primary" data-toggle="collapse" href="#collapseExample1" aria-expanded="false" aria-controls="collapseExample1"> PAID TICKET</button></p>
        <div class="collapse" id="collapseExample1">
          <div class="well">
          <label for="exampleInputName1">Ticket name</label>
          <input type="text" class="form-control" id="exampleInputName1" placeholder="Cinderella">
          <label for="exampleInputName2">Price</label>
          <input type="text" class="form-control" id="exampleInputName2" placeholder="$   15">
          </div>
        </div>
      </div>
    <div class="col-sm-3">
      <p></p>
      <p><button class="btn btn-primary" data-toggle="collapse" href="#collapseExample3" aria-expanded="false" aria-controls="collapseExample3"> DONATION</button></p>
        <div class="collapse" id="collapseExample3">
          <div class="well">
          <label for="exampleInputName1">Ticket name</label>
          <input type="text" class="form-control" id="exampleInputName1" placeholder="Snow White">
          <label for="exampleInputName3">Price</label>
          <input type="text" class="form-control" id="exampleInputName3" placeholder="$   19">
          </div>
        </div>
    </div>
  </div>
