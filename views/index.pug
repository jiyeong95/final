block hero-unit
  #carouselFade.carousel.slide.carousel-fade(data-ride='carousel', data-interval='2000')
    .carousel-inner(role='listbox')
        .carousel-item.active
          img.d-block.w-100(src='https://cdn.evbstatic.com/s3-build/perm_001/35d88f/django/images/home/banners/homepage_hero_banner_1.jpg', height='410px', alt='First slide')
        .carousel-item
          img.d-block.w-100(src='https://cdn.evbstatic.com/s3-build/perm_001/aa36c3/django/images/home/banners/homepage_hero_banner_2.jpg', height='410px', alt='Second slide')
        .carousel-item
          img.d-block.w-100(src='https://cdn.evbstatic.com/s3-build/perm_001/6812b9/django/images/home/banners/homepage_hero_banner_3.jpg', height='410px', alt='Third slide')
        .carousel-item
          img.d-block.w-100(src='https://cdn.evbstatic.com/s3-build/perm_001/99ca9f/django/images/home/banners/homepage_hero_banner_4.jpg', height='410px', alt='Fourth slide')

block jmb
  .jumbotron.jumbotron-fluid
    .container
      h1.display Find your next experience
      p.lead
        form.form-inline
          .btn-group(role='group', aria-label='Button group with nested dropdown')
            input.form-control.mr-sm-2(type='text', placeholder='Search events or categories', aria-label='Search')
            input.form-control.mr-sm-2(type='text', placeholder='City or location', value='Seoul, Republic of Korea', aria-label='Search')
            .dropdown
              button.btn2.btn-light.dropdown-toggle(type='button', data-toggle='dropdown') All Dates
              .dropdown-menu
                a.dropdown-item(href='#') All Dates
                a.dropdown-item(href='#') Today
                a.dropdown-item(href='#') Tomorrow
                a.dropdown-item(href='#') This Week
                a.dropdown-item(href='#') This Weekend
                a.dropdown-item(href='#') Next Week
                a.dropdown-item(href='#') Next Month
                
            button.btn1.btn-light(type='submit') Search
            .dropdown-menu
              a.dropdown-item(href='#') Blog
              a.dropdown-item(href='#') Resources
              a.dropdown-item(href='#') Fees
              a.dropdown-item(href='#') How it works
              a.dropdown-item(href='#') Features

